{
    "name": "madebyprisma/better-link",
    "version": "4.12.0",
    "description": "A model & field for linking to pages, or external URLs in SilverStripe.",
    "authors": [
        {
            "name": "Jesse Solomon",
            "email": "jesse@madebyprisma.com"
        }
    ],
    "require": {
        "stevie-mayhew/hasoneedit": "^2.0",
        "silverstripe/recipe-cms": "~4.12.0@stable"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        }
    },
    "extra": {
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    }
}
